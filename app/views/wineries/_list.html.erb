<table class="table table-hover">
  <thead>
  <tr>
    <th>Name</th>
    <th>Year</th>
  </tr>
  </thead>

  <tbody>
  <% wineries.each do |winery| %>
      <tr>
        <td><%= link_to winery.name, winery %></td>
        <td><%= winery.year %></td>
        <td></td>
      </tr>
  <% end %>
  </tbody>
</table>