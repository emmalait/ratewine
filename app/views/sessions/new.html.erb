<p id="notice"></p>

<h1>Sign in</h1>

<p><%= form_tag session_path do %>
  username <%= text_field_tag :username, params[:username] %>
  password <%= password_field_tag :password, params[:password] %>
  <%= submit_tag "Log in" %>
<% end %></p>

<p><%= link_to "Sign in with Github", "auth/github/", method: :get, class: "btn btn-primary" %></p>